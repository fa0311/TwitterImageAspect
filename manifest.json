{
    "name": "TwitterImageAspect",
    "description": "Twitterの画像のアスペクト比を3:4にする拡張機能です",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "html/popup.html"
    },
    "content_scripts": [{
        "matches": [
            "https://twitter.com/*"
        ],
        "js": [
            "js/lib/jquery-3.6.0.min.js",
            "js/main.js"
        ],
        "css": [
            "css/main.css"
        ]
    }]
}